page {
  font-family: "PingFang SC";
}

.header {
  /* background: #ffc740; */
  padding: 26rpx;
  background: -webkit-linear-gradient(bottom, rgb(253, 98, 51), rgb(237, 99, 76)); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(bottom, rgb(253, 98, 51), rgb(237, 99, 76)); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(bottom, rgb(253, 98, 51), rgb(237, 99, 76)); /* Firefox 3.6 - 15 */
  background: linear-gradient(to bottom, rgb(253, 98, 51), rgb(237, 99, 76)); /* 标准的语法 */
}

.header .avatar {
  display: block;
  margin: 0 auto;
  width: 156rpx;
  height: 156rpx;
  border-radius: 50%;
  border: 2px solid #fee767;
}

.header .nickname {
  color: #fff;
  font-size: 22px;
  line-height: 2.4;
  text-align: center;
}

.section {
  padding: 0;
  color: #2e2e2e;
  font-size: 30rpx;
}

.dayu {
  height: 25rpx;
  width: 16rpx;
  margin-right: -12rpx;
  display: inline-block;
  vertical-align: middle;
}

.fuwu {
  height: 26rpx;
  width: 28rpx;
}

.section .line {
  padding-left: 12rpx;
  padding-right: 30rpx;
  line-height: 3;
  background-color: #fff;
}

.section .us {
  padding-left: 12rpx;
  padding-right: 30rpx;
  line-height: 3;
  border-bottom: 1px solid #efefef;
  background-color: #fff;
}

.section .line:last-child {
  border-bottom: none;
}

.section .line .tl {
  color: #aaa;
  font-size: 24rpx;
  margin-left: 10rpx;
}

.container {
  height: 60%;
  font-family: "PingFang SC";
}

scroll-view {
  height: 100%;
}

.header.scrolled {
  background: #feb70f;
}

.header .icon {
  width: 40rpx;
  height: 40rpx;
  vertical-align: top;
}

.icons {
  height: 180rpx;
  padding: 16rpx 10rpx;
  padding-bottom: 0;
}

.icons swiper-item {
  width: 100%;
  /*display: -webkit-flex;*//*flex-direction: row;*//*flex-wrap: wrap;*//*justify-content: center;*/
}

.icons .icon {
  float: left;
  display: -webkit-flex;
  flex-direction: column;
  flex-wrap: wrap;
  width: 25%;
  text-align: center;
  margin: 10rpx 0;
}

.icons .icon image {
  margin: 0 auto;
  width: 100rpx;
  height: 100rpx;
}

.icons .icon text {
  color: #000;
  font-size: 24rpx;
  line-height: 2;
}

.section-big {
  padding: 20rpx;
}

.section-big .title {
  position: relative;
  text-align: center;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 2;
  margin-bottom: 20rpx;
}

.section-big .title:before {
  content: " ";
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: -180rpx;
  width: 100rpx;
  height: 1rpx;
  background: #000;
}

.section-big .title:after {
  content: " ";
  position: absolute;
  left: 50%;
  top: 50%;
  margin-left: 80rpx;
  width: 100rpx;
  height: 1rpx;
  background: #000;
}

.discount .content {
  width: 100%;
  height: 450rpx;
}

.preferred .content {
  width: 100%;
  height: 556rpx;
}

.filters {
  display: -webkit-flex;
}

.tfilter {
  height: 60rpx;
}

.filters .filter {
  -webkit-flex: 1;
  text-align: center;
  font-size: 34rpx;
  color: red;
  line-height: 2;
}

.filters .filter1 {
  -webkit-flex: 1;
  text-align: center;
  font-size: 26rpx;
  color: #757575;
  line-height: 2;
}

.special .content {
  width: 100%;
  height: 181rpx;
}

.loading {
  font-size: 32rpx;
  text-align: center;
  line-height: 2;
  margin-bottom: 20rpx;
}

.mineimages1 {
  width: 42rpx;
  height: 42rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 12rpx;
  font-size: 34rpx;
}

.look {
  margin-left: 280rpx;
  color: rgb(136, 136, 136);
  font-size: 27rpx;
}

.mineimages2 {
  width: 30rpx;
  height: 80rpx;
  display: inline-block;
  vertical-align: middle;
  margin-right: 12rpx;
}
.service{
  font-size:31rpx;
  color:#151515;
  padding-bottom: 28rpx;
  padding-top: 28rpx;
}
.support{
  font-size: 24rpx;
  color: #aaa;
  position: fixed;
  bottom: 20rpx;
  width: 100%;
  text-align: center;
}