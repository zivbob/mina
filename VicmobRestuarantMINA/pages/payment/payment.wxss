/* payment.wxss */

page {
  height: 100%;
}

.container {
  height: 100%;
  font-family: "PingFang SC";
}

.addresslist {
  width: 100%;
  border-bottom: 1px solid #e8e8e8;
}

.address {
  font-size: 30rpx;
  color: #606060;
  height: 150rpx;
  margin-top: 18rpx;
  background: #fff url(data:image/jpeg;base64,) repeat-x bottom;
  background-size: auto 1px;
}

.location {
  width: 80%;
  margin-left: 20rpx;
  padding-left: 50rpx;
  height: 140rpx;
  float: left;
  background: url(data:image/jpeg;base64,) no-repeat left center;
  background-size: auto 50rpx;
}

.locationL {
  width: 10%;
  height: 150rpx;
  float: right;
  background: url(data:image/jpeg;base64,) no-repeat center;
  background-size: auto 30rpx;
}

.lotit {
  font-size: 26rpx;
  color: #4a4a4a;
  margin-top: 26rpx;
}

.lotit1 {
  width: 100%;
  height: 100%;
  font-size: 32rpx;
  color: #4a4a4a;
  display: flex;
  align-items: center;
}

.lotxt {
  font-size: 28rpx;
  color: #4a4a4a;
  margin-top: 12rpx;
}

.list {
  background: #fff;
  margin-top: 20rpx;
}

.list .item {
  padding: 24rpx 16rpx;
  font-size: 28rpx;
  color: #333;
  position: relative;
  border-bottom: 1rpx solid #f3f3f3;
}

.list .item::last-child {
  border: none;
}

.paytype {
  background: #fff;
  padding: 30rpx 20rpx;
  font-size: 32rpx;
  color: #4a4a4a;
}

.paytype1 {
  float: right;
  margin-right: 10rpx;
  font-size: 28rpx;
  color: #a5a5a5;
}

.paytype2 {
  float: right;
  margin-right: 10rpx;
  font-size: 28rpx;
  color: #000;
}

.detaillist {
  background: #fff;
  margin-top: 30rpx;
  padding: 10rpx 0 0;
}

.detaillist .table {
  width: 100%;
  font-size: 30rpx;
  color: #606060;
  text-align: center;
  border-bottom: 1px solid #e8e8e8;
}

.tr {
  display: flex;
  padding: 15rpx 20rpx;
  text-align: center;
  border-bottom: 1rpx solid #f3f3f3;
}

.th, .td {
  border: 0;
  width: 100%;
}

.ordername {
  width: 70%;
  text-align: left;
}

.ordernum {
  width: 15%;
}

.orderoriginalprice {
  width: 15%;
  text-decoration: line-through;
  font-size: 26rpx;
  text-align: center;
  color: gray;
}

.orderprice {
  width: 15%;
  text-align: right;
}

.trbom {
  display: flex;
  padding: 15rpx 20rpx;
}

.tdbomtit {
  text-align: left;
  width: 85%;
  font-size: 28rpx;
}

.tdbomprice {
  text-align: right;
  width: 15%;
  color: #f38815;
}

.tabletotal0 {
  font-size: 30rpx;
  color: #4a4a4a;
  text-align: right;
  padding: 20rpx;
}

.tabletotal1 {
  display: inline;
  font-size: 30rpx;
  color: #606060;
  margin-right: 10px;
}

.tabletotal2 {
  display: inline;
  font-size: 30rpx;
  color: #606060;
  margin-right: 10px;
}

.tabletotal3 {
  display: inline;
  font-size: 36rpx;
  color: #f38815;
}

.paybom {
  position: fixed;
  left: 0;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  background: #3c3d41;
  z-index: 999999;
}

.paybtn {
  width: 200rpx;
  height: 100%;
  font-size: 30rpx;
  background: #f38815;
  color: #fff;
  line-height: 100rpx;
  border-radius: 0;
  float: right;
  display: inline-block;
}

.paybom1 {
  font-size: 30rpx;
  color: #fff;
  display: inline-block;
  float: left;
  line-height: 100rpx;
  margin-left: 10px;
}

.paybom2 {
  font-size: 24rpx;
  color: #eee;
  display: inline-block;
  float: left;
  line-height: 100rpx;
  margin-left: 5px;
}
