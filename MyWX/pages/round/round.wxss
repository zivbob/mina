page {
  width: 100%;
  height: 100%;
}
.container {
  width: 100%;
  height: 100%;
}

@keyframes round{100% {transform: rotateZ(360deg)}}
.circle {
  width: 400rpx;
  height: 400rpx;
  border-radius: 50%; 
  background-color: gray;
  margin-top: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  animation: round 1s linear infinite;  
}