 @keyframes kf-marque-animation{ 0% { transform: translateX(100%); } 100% { transform: translateX(-100%); } } 
.marquee{
    width: 100%;
    height: 44px;
    line-height: 44px;
    background: gray;
    border: none;
    display: block;
    margin: 0 auto;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: clip;
    font-size: 28rpx;
}
.content{
    justify-content: space-between;
    padding-right: 0px;
    min-width: 100%;
    animation: kf-marque-animation 5s linear infinite;
    white-space: nowrap;
}

@keyframes round{ from {transform: rotate(0deg)} to{transform: rotate(360deg)}}
.squre{
  width: 100rpx;
  height: 100rpx;
  background-color: #3BA3D0;
  margin: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  animation: round 1s linear infinite; 
}

.squre2{
  width: 50rpx;
  height: 50rpx;
  background-color: #ff0700;
  display: flex;
  align-items: center;
  justify-content: center;
  align-content: center;
  animation: round 500ms linear infinite; 
}

@keyframes GROUND{ from {background-color:black; transform: rotateY(0deg) scale(1) } to{background-color:#fff; transform: rotateY(360deg) scale(2)}}
.round {
  width: 200rpx;
  height: 200rpx;
  background-color: #A600A6;
  margin: 100rpx 300rpx;
  border-radius: 50%;
  animation: GROUND 1s linear infinite alternate;
}

@keyframes GROUND2{ from {transform: rotateZ(0deg)} to{transform: rotateZ(360deg)}}
.round2 {
  width: 200rpx;
  height: 200rpx;
  background-color: #A600A6;
  margin: 100rpx;
  animation: GROUND2 1s linear infinite;
}

@keyframes rock{from {transform: rotate(0deg)} to{transform: rotate(360deg)}}
.taiji {
  margin: 100rpx 50rpx;
  width: 600rpx;
  height: 600rpx;
  background: linear-gradient(to right, #000 50%,#fff 50%);
  border: 1rpx solid;
  border-radius: 50%;
  display: flex;
  flex-direction: column;
  animation: rock 2s linear infinite;
}
.out {
  width: 50%;
  height: 50%;
  background-color: #000;
  border-radius: 50%;
}
.left {
  margin-left: 25%;
  background-color: #000;
}
.right {
  margin-left: 25%;
  background-color: #fff;
}

@keyframes run{from {transform: rotateX(0deg)} to{transform: rotateX(360deg)}}
.little {
  width: 25%;
  height: 25%;
  border-radius: 50%;
  margin: 25%;
  animation: shine 500ms linear infinite alternate;
}

@keyframes shine{from {background-color: #fff} to{background-color: #000}}
.white {
  background-color: #fff;
}
.black {
  background-color: #000;
}

.box {
  width:750rpx;
  height: 750rpx;
  /* background: repeating-linear-gradient(45deg, #000 0%, #fff 5%); */
  background: radial-gradient( 750rpx, #f00, #ff0, #080);
  border-radius: 50%;
}