page {
  width: 100%;
  height: 100%;
}

.contain {
  width: 100%;
  height: 100%;
}
.moveBox {
  position: fixed;
  height: 800rpx;
  top: 0;
  right: 70rpx;
}
.move {
  width: 100rpx;
  height: 100rpx;
  background-color: #33cccc;
}
.col {
  top: 80rpx;
  background-color: #476bd6;
}
.textBox {
  position: absolute;
}
.test {
  width: 1000rpx;
  height: 100rpx;
  background-color: #ff6400;
}
.ccc {
  background-color: #476bd6;
}
.btn {
  width: 1000rpx;
  height: 100rpx;
  background-color: #4284d3;
  border-radius: 25rpx;
}
.frog {
  width: 100%;
  height: 100%;
  position: fixed;
  opacity: 0.5;
  background-color: #000;
  top: 0;
  z-index: 1;
}